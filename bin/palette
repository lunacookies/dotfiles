#!/bin/sh

printf "\n"
for i in $(seq 0 7); do
	printf "  \e[4%dm    \e[m" "$i"
done
printf "\n"
for i in $(seq 0 7); do
	printf "  \e[4%dm    \e[m" "$i"
done
printf "\n\n"
for i in $(seq 0 7); do
	printf "  \e[10%dm    \e[m" "$i"
done
printf "\n"
for i in $(seq 0 7); do
	printf "  \e[10%dm    \e[m" "$i"
done
printf "\n\n"
