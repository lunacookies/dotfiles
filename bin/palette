#!/bin/dash

printf "\n"
for i in $(seq 0 7); do
	printf "  \033[4%dm    \033[m" "$i"
done
printf "\n"
for i in $(seq 0 7); do
	printf "  \033[4%dm    \033[m" "$i"
done
printf "\n\n"
for i in $(seq 0 7); do
	printf "  \033[10%dm    \033[m" "$i"
done
printf "\n"
for i in $(seq 0 7); do
	printf "  \033[10%dm    \033[m" "$i"
done
printf "\n\n"
